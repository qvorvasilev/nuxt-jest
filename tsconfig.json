{
  "compilerOptions": {
    "moduleResolution": "Node",
    "target": "ES2018",
    "module": "ESNext",
    //    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/axios",
      "@types/node",
      "@types/jest",
      "@nuxtjs/vuetify",
      "@vue/test-utils",
      //      "@types/vuelidate",
      //      "vuetify-dialog/types",
      "@nuxtjs/i18n",
      "cookie-universal-nuxt",
      "node",
    ]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
